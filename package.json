{
  "name": "the-agentic-startup",
  "version": "1.0.0",
  "description": "Enterprise-grade AI development agents as frictionless as any npm package",
  "type": "module",
  "author": "The Startup Team",
  "license": "MIT",
  "keywords": [
    "ai",
    "claude",
    "agents",
    "development",
    "tui",
    "cli",
    "specification",
    "workflow"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/the-startup/the-agentic-startup.git"
  },
  "bugs": {
    "url": "https://github.com/the-startup/the-agentic-startup/issues"
  },
  "homepage": "https://github.com/the-startup/the-agentic-startup#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "the-agentic-startup": "./dist/index.js",
    "the-startup-statusline": "./bin/statusline.sh"
  },
  "files": [
    "dist/",
    "bin/",
    "assets/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "prepublishOnly": "npm run clean && npm run build && npm run typecheck && npm test"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "ink": "^4.4.1",
    "ink-text-input": "^5.0.1",
    "inquirer": "^10.2.2",
    "ora": "^8.1.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.18.0",
    "ink-testing-library": "^4.0.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
